{
  "name": "maana-my-service",
  "author": "Maana, Inc.",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Maana Q Knowledge Service Template",
  "source": "src/server.js",
  "main": "dist/server.js",
  "repository": "git@github.com:maana-io/q-svc-templates.git",
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "scripts": {
    "docker-build": "docker build -t maana-service-js .",
    "docker-run": "docker run -p 8050:8050 maana-service-js",
    "format": "prettier --write \"src/**/*.js\"",
    "start": "node ./dist/server.js",
    "dev": "nodemon --exec babel-node --presets @babel/preset-env ./src/server.js",
    "generate": "graphql-codegen --config codegen.yml",
    "build": "parcel build"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "apollo-server": "^3.6.2",
    "apollo-server-core": "^3.6.2",
    "dotenv": "^15.0.0",
    "graphql": "^16.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "nodemon": "^2.0.15",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1"
  }
}
